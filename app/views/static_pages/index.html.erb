<h2>Flickr Images</h2>

<div>
  <%= form_with url: '/', method: 'get' do |f| %>
    <%= f.label :user_id %>
    <%= f.text_field :user_id, placeholder: 'User ID' %>
  
    <%= f.submit "Find Photos" %>
  <% end %>
</div><br>

<% @photos.each do |photo| %>
    <img src="<%= Flickr.url_n(photo) %>"/>
<% end %> 
